{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-trailing-spaces": ["error", { "skipBlankLines": true }],
        "no-console": "off",
        "eol-last": ["error", "always"],
        "new-cap": ["error", { "newIsCap": true }],
        "eqeqeq": "warn",
        "indent": ["error", 2],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "max-len": ["error", { "code": 100, "ignoreComments": true }]
    }
}
